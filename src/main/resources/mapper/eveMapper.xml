<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="event">


	<select id="all"  resultType="eveVO" >
		select * from EVENT
	</select>
	
	<select id="one" parameterType="eveVO" resultType="eveVO">
		select * from EVENT where eveid = #{eveid}
	</select>
	
	
</mapper>